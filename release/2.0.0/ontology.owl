        <rdfs:range rdf:resource="https://w3id.org/hto#FeatureType"/>
        <rdfs:comment xml:lang="en">hasFeatureType links a spatial feature to a hto:FeatureType instance indicating its category, such as city, county, or street.</rdfs:comment>
        <rdfs:label xml:lang="en">hasFeatureType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hto#hasOriginalDescription -->


