        <rdfs:range rdf:resource="https://w3id.org/hto#OriginalDescription"/>
        <rdfs:comment xml:lang="en">Links a Work specifically to its OriginalDescription, representing the primary or source textual content of that work as found in a digitised collection.</rdfs:comment>
        <rdfs:label xml:lang="en">hasOriginalDescription</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hto#hasSummary -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hto#hasSummary">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/hto#hasDescription"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/hto#Description"/>
                    <rdf:Description rdf:about="https://w3id.org/hto#Work"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/hto#Summary"/>
        <rdfs:comment xml:lang="en">Links either a Work or a Description to a Summary that provides a condensed representation of its content. This property is used to associate manual or automatically generated summaries with the texts they describe.</rdfs:comment>
        <rdfs:label xml:lang="en">hasSummary</rdfs:label>
    </owl:ObjectProperty>
    


