        <rdfs:range rdf:resource="https://w3id.org/hto#Description"/>
        <rdfs:comment xml:lang="en">Links a Work to a Description that provides textual content or a textual representation of that work. This includes original texts, summaries, or other derived descriptions associated with a historical work.</rdfs:comment>
        <rdfs:label xml:lang="en">hasDescription</rdfs:label>
    </owl:ObjectProperty>
    


