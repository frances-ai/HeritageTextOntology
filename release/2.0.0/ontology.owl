    <!-- https://w3id.org/hto#subtitle -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/hto#subtitle">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:domain rdf:resource="https://w3id.org/hto#Work"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">is the secondary title for a work</rdfs:comment>
        <rdfs:label xml:lang="en">subtitle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hto#termsOfAddress -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/hto#termsOfAddress">
        <rdfs:domain rdf:resource="https://schema.org/Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">records one or more terms of address associated with a person, such as titles or honorifics (“Dr”, “Prof.”, “Sir”, etc.).</rdfs:comment>
        <rdfs:label xml:lang="en">termsOfAddress</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hto#text -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/hto#text">
        <rdfs:domain rdf:resource="https://w3id.org/hto#Description"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">is the literal text content of a Description as a string. This property holds the actual characters of original texts, summaries, or other descriptive content associated with a Description instance.</rdfs:comment>
        <rdfs:label xml:lang="en">text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hto#title -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/hto#title">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/title"/>
        <rdfs:domain rdf:resource="https://w3id.org/hto#Work"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">is the title of a work.</rdfs:comment>
        <rdfs:label xml:lang="en">title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/hto#yearPublished -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/hto#yearPublished">
        <rdfs:domain rdf:resource="https://w3id.org/hto#Work"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/Agent -->


    <owl:Class rdf:about="http://purl.org/dc/terms/Agent"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E53_Place -->


    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E55_Type -->


    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
    


    <!-- http://www.opengis.net/ont/geosparql#Feature -->


    <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#Feature"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#Concept -->


    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
    


    <!-- http://www.w3.org/ns/oa#Annotation -->


    <owl:Class rdf:about="http://www.w3.org/ns/oa#Annotation"/>
    


    <!-- http://www.w3.org/ns/oa#SpecificResource -->


    <owl:Class rdf:about="http://www.w3.org/ns/oa#SpecificResource"/>
    


    <!-- http://www.w3.org/ns/org#OrganizationalUnit -->


    <owl:Class rdf:about="http://www.w3.org/ns/org#OrganizationalUnit"/>
    


    <!-- http://www.w3.org/ns/prov#Entity -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Entity"/>
    


    <!-- http://www.w3.org/ns/prov#Location -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Location"/>
    


    <!-- http://xmlns.com/foaf/0.1/Organization -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person"/>
    


    <!-- https://schema.org/Person -->


    <owl:Class rdf:about="https://schema.org/Person"/>
    


    <!-- https://w3id.org/hto#Agent -->


    <owl:Class rdf:about="https://w3id.org/hto#Agent">
        <vann:example>hto:dugald a hto:Agent, schema:Person;
  foaf:name &quot;Dugald Stewart&quot;^^xsd:string;
  hto:birthYear &quot;1753&quot;^^xsd:integer;
  hto:deathYear &quot;1828&quot;^^xsd:integer;
  hto:termsOfAddress &quot;Prof.&quot;^^xsd:string.

hto:edition3 a hto:Edition, schema:CreativeWork;
  hto:title &quot;Encyclopaedia Britannica&quot;;
  hto:number &quot;3&quot;^^xsd:integer;
  schema:editor hto:dugald.</vann:example>
        <rdfs:comment xml:lang="en">An Agent represents a historical individual or collective entity that plays an active or attributable role in relation to historical documents. Agents may be persons, organisations, institutions, or communities involved in the creation, production, dissemination, curation, or use of historical texts. They can also appear within texts as referenced or represented actors, such as authors, editors, correspondents, or subjects.</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#AuthorityType -->


    <owl:Class rdf:about="https://w3id.org/hto#AuthorityType">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
        <vann:example>&lt;https://www.wikidata.org/wiki/Q203000&gt; a hto:ExternalRecord;
  hto:hasAuthorityType hto:WikidataItem.

hto:perth_scotland a skos:Concept, hto:Location;
  rdfs:label &quot;Perth&quot;^^xsd:integer;
  hto:hasConceptRecord &lt;https://www.wikidata.org/wiki/Q203000&gt;.</vann:example>
        <rdfs:comment xml:lang="en">AuthorityType represents a controlled set of types used to classify resources according to the authority or knowledge base they are associated with. Instances of this class indicate whether a resource corresponds to, for example, a Wikidata item, DBpedia resource, GeoNames feature, or other authority record. hto:AuthorityType is intended to be used with hto:hasAuthorityType on entities linked to external identifiers, without introducing separate classes for each authority source.</rdfs:comment>
        <rdfs:label xml:lang="en">AuthorityType</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Broadside -->


    <owl:Class rdf:about="https://w3id.org/hto#Broadside">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Work"/>
        <rdfs:comment xml:lang="en">A broadside is a single-sheet printed publication used historically to disseminate proclamations, ballads, crime reports, advertisements, and other public communications.</rdfs:comment>
        <rdfs:label xml:lang="en">Broadside</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#ConceptRecord -->


    <owl:Class rdf:about="https://w3id.org/hto#ConceptRecord">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#Description"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#OriginalDescription"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#Summary"/>
        <vann:example>&lt;https://www.wikidata.org/wiki/Q203000&gt; a hto:ExternalRecord, hto:ConceptRecord;
  hto:hasAuthorityType hto:WikidataItem.

hto:perth_scotland_eb2 a hto:LocationRecord, hto:ConceptRecord;
  hto:startsAtPage hto:eb2_vol8_page485;
  hto:endsAtPage hto:eb2_vol8_page485.

hto:perth_scotland_eb3 a hto:LocationRecord, hto:ConceptRecord;
  hto:startsAtPage hto:eb3_vol8_page22;
  hto:endsAtPage hto:eb3_vol8_page22.

hto:perth_scotland_concept a skos:Concept;
  hto:hasConceptRecord &lt;https://www.wikidata.org/wiki/Q203000;
  hto:hasConceptRecord hto:perth_scotland_eb2;
hto:hasConceptRecord hto:perth_scotland_eb3.</vann:example>
        <rdfs:comment xml:lang="en">A Concept Record is a specific record in a historical document or a modern work that descirbe a skos:concept. One concept might have multiple concept records. It is used to link records about the same concept across time and sources.</rdfs:comment>
        <rdfs:label xml:lang="en">ConceptRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Description -->


    <owl:Class rdf:about="https://w3id.org/hto#Description">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#ExternalRecord"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#InternalRecord"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#LocationRecord"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#TermRecord"/>
        <rdfs:comment xml:lang="en">A Description represents a textual or textual-like representation of content associated with a historical work. Instances of this class include original texts, summaries, and other derived descriptions that can be linked to provenance, quality.</rdfs:comment>
        <rdfs:label xml:lang="en">Description</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Edition -->


    <owl:Class rdf:about="https://w3id.org/hto#Edition">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Work"/>
        <vann:example>hto:eb7 a hto:Work hto:Edition, schema:CreativeWork;
   hto:mmsid &quot;9910796273804340&quot;^^xsd:string;
   hto:title &quot;Encyclopaedia Britannica&quot;^^xsd:string;
   hto:number &quot;7&quot;^^xsd:integer;
   hto:yearPublished &quot;1842&quot;^^xsd:integer;
   schema:hasPart hto:eb7_vol1.

hto:eb7_vol1 a hto:Work hto:Volume, schema:CreativeWork;
  hto:number &quot;1&quot;^^xsd:integer;
  hto:title &quot;Seventh edition, Volume 1, Preliminary dissertations&quot;^^xsd:string;
  dct:identifier &quot;192984258&quot;^^xsd:string;
  hto:numberOfPage &quot;942&quot;^^xsd:integer.</vann:example>
        <rdfs:comment xml:lang="en">An edition is a particular version or iteration of a specific work. For example, the encyclopaedia collection has serveral editions, each representing a revised or updated version of the original work.</rdfs:comment>
        <rdfs:label xml:lang="en">Edition</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#ExternalRecord -->


    <owl:Class rdf:about="https://w3id.org/hto#ExternalRecord">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#ConceptRecord"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#OriginalDescription"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#Summary"/>
        <vann:example>&lt;https://www.wikidata.org/wiki/Q203000&gt; a hto:ExternalRecord, hto:ConceptRecord;
  hto:hasAuthorityType hto:WikidataItem.

hto:perth_scotland_eb2 a hto:LocationRecord, hto:ConceptRecord;
  hto:startsAtPage hto:eb2_vol8_page485;
  hto:endsAtPage hto:eb2_vol8_page485.

hto:perth_scotland_concept a skos:Concept;
  hto:hasConceptRecord &lt;https://www.wikidata.org/wiki/Q203000;
  hto:hasConceptRecord hto:perth_scotland_eb2.</vann:example>
        <rdfs:comment xml:lang="en">An ExternalRecord represents a concept record that originates from an external authority or knowledge base, such as Wikidata, DBpedia, or GeoNames. It is used to link concepts and internal records to corresponding external sources.</rdfs:comment>
        <rdfs:label xml:lang="en">ExternalRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#FeatureType -->


    <owl:Class rdf:about="https://w3id.org/hto#FeatureType">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
        <vann:example>hto:perth_scotland a hto:Location, geo:Feature, crm:Place;
  rdfs:label &quot;Perth&quot;^^xsd:integer;
  hto:hasFeatureType hto:City;
  crm:P89_falls_within hto:scotland.

hto:scotland a hto:Location, geo:Feature, crm:Place;
  rdfs:label &quot;Scotland&quot;^^xsd:integer;
  hto:hasFeatureType hto:Country.</vann:example>
        <rdfs:comment xml:lang="en">FeatureType represents a controlled set of types used to categorise geographic or spatial features, such as city, county, village, river, or street.</rdfs:comment>
        <rdfs:label xml:lang="en">FeatureType</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#InternalRecord -->


    <owl:Class rdf:about="https://w3id.org/hto#InternalRecord">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#ConceptRecord"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#OriginalDescription"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#Summary"/>
        <rdfs:comment xml:lang="en">An InternalRecord represents a concept record derived from within a textual heritage collection, such as an encyclopaedia article, dictionary entry, or gazetteer entry. It captures how a concept is described in a particular historical source and can be related to pages, descriptions, and external records.</rdfs:comment>
        <rdfs:label xml:lang="en">InternalRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#LocationRecord -->


    <owl:Class rdf:about="https://w3id.org/hto#LocationRecord">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#InternalRecord"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#OriginalDescription"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#Summary"/>
        <vann:example>hto:perth_scotland_eb2 a hto:LocationRecord, hto:TermRecord;
  hto:startsAtPage hto:eb2_vol8_page485;
  hto:endsAtPage hto:eb2_vol8_page485;
  hto:hasOriginalDescription hto:perth_scotland_eb2_desc;
  hto:describesPlace hto:perth_scotland_1778;
  hto:refersToModernPace hto:perth_scotland_2025.

hto:perth_scotland_eb2_desc a hto:OriginalDescription;
  hto:text &quot;a county of Scotland, ...&quot;^^xsd:string;
  hto:hasTextQuality hto:Low.

hto:perth_scotland_1778 a geo:Feature, crm:Place;
  rdfs:label &quot;Perth&quot;^^xsd:integer;
  hto:hasFeatureType hto:County;

hto:perth_scotland_2025 a geo:Feature, crm:Place;
  rdfs:label &quot;Perth&quot;^^xsd:integer;
  hto:hasFeatureType hto:City;</vann:example>
        <rdfs:comment xml:lang="en">A LocationRecord represents that any internal record that provides a description, definition, or explanation of a specific location.</rdfs:comment>
        <rdfs:label xml:lang="en">LocationRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#OriginalDescription -->


    <owl:Class rdf:about="https://w3id.org/hto#OriginalDescription">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Description"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#TermRecord"/>
        <vann:example>hto:perth_scotland_eb2 a hto:LocationRecord, hto:TermRecord;
  hto:hasOriginalDescription hto:perth_scotland_eb2_desc;

hto:perth_scotland_eb2_desc a hto:OriginalDescription;
  hto:text &quot;a county of Scotland, ...&quot;^^xsd:string;
  hto:hasTextQuality hto:Low.</vann:example>
        <rdfs:comment xml:lang="en">An OriginalDescription represents the original textual content of a historical document or work as it appears in a given source or digitisation workflow (e.g. OCR output or manually transcribed text). It serves as the primary reference text from which other descriptions, summaries, or annotations may be derived.</rdfs:comment>
        <rdfs:label xml:lang="en">OriginalDescription</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Page -->


    <owl:Class rdf:about="https://w3id.org/hto#Page">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Work"/>
        <rdfs:comment xml:lang="en">A Page represents a single page that forms part of a hto:Work.</rdfs:comment>
        <rdfs:label xml:lang="en">Page</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Series -->


    <owl:Class rdf:about="https://w3id.org/hto#Series">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Work"/>
        <vann:example>hto:gaz1825 a hto:Work hto:Series, schema:CreativeWork;
   hto:mmsid &quot;9931344583804340&quot;^^xsd:string;
   hto:title &quot;Gazetteer of Scotland&quot;^^xsd:string;
   hto:number &quot;7&quot;^^xsd:integer;
   hto:yearPublished &quot;1825&quot;^^xsd:integer;
   schema:hasPart hto:gaz1825_vol1.

hto:gaz1825_vol1 a hto:Work hto:Volume, schema:CreativeWork;
  hto:number &quot;1&quot;^^xsd:integer;
  hto:title &quot;Gazetteer of Scotland; arranged under the various descriptions of counties, parishes, islands 1825&quot;^^xsd:string;
  dct:identifier &quot;97421702&quot;^^xsd:string;
  hto:numberOfPage &quot;222&quot;^^xsd:integer.</vann:example>
        <rdfs:comment xml:lang="en">A series is a group of related works, typically but not necessarily of the same kind. Series are usually organized into some order, often chronological.</rdfs:comment>
        <rdfs:label xml:lang="en">Series</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Summary -->


    <owl:Class rdf:about="https://w3id.org/hto#Summary">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Description"/>
        <owl:disjointWith rdf:resource="https://w3id.org/hto#TermRecord"/>
        <rdfs:comment xml:lang="en">A Summary represents a derived, condensed description of a work or original description, typically produced by manual or automated summarisation.</rdfs:comment>
        <rdfs:label xml:lang="en">Summary</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#TermRecord -->


    <owl:Class rdf:about="https://w3id.org/hto#TermRecord">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#InternalRecord"/>
        <rdfs:comment xml:lang="en">A TermRecord represents an article or entry in a dictionary-like textual work that provides a description, definition, or explanation of a specific term. It applies to structured reference works such as encyclopaedias, dictionaries, lexicons, or descriptive gazetteers, where each entry corresponds to a named concept—e.g., a place, person, event, or general topic. A hto:TermRecord captures the conceptual focus of the entry (the term), its associated textual content, and any metadata or provenance relating to its creation, publication, or digitisation.</rdfs:comment>
        <rdfs:label xml:lang="en">TermRecord</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#TextQuality -->


    <owl:Class rdf:about="https://w3id.org/hto#TextQuality">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
        <rdfs:comment xml:lang="en">TextQuality represents a type or category used to characterise the quality of a textual description</rdfs:comment>
        <rdfs:label xml:lang="en">TextQuality</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#TextSegment -->


    <owl:Class rdf:about="https://w3id.org/hto#TextSegment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/oa#SpecificResource"/>
        <vann:example>hto:OriginalDescription_Perth_1823 a hto:OriginalDescription ;
  hto:title &quot;Perth&quot; ;
  hto:text &quot;Perth, a county in Scotland ...&quot;^^xsd:string; .

hto:Segment_Scotland_001 a hto:TextSegment ;
  oa:hasSource hto:OriginalDescription_Perth_1842 ;
  oa:hasSelector [
    a oa:TextPositionSelector ;
    oa:start 19 ;
    oa:end   27
  ] .

hto:Annotation_Scotland_001 a oa:Annotation ;
  oa:hasTarget hto:Segment_Scotland_001 ;
  oa:hasBody hto:Location_Scotland.

hto:Location_Scotland a crm:E53_Place.</vann:example>
        <rdfs:comment xml:lang="en">A TextSegment represents a specific segment of text within an hto:OriginalDescription. It is used to identify and describe spans such as persons, locations, events, dates, or other textual fragments produced by NER or other NLP tasks. As a subclass of oa:SpecificResource, a hto:TextSegment points to its source text and is refined by a selector (e.g. character offsets, quoted text). Annotations can then attach identified entities or NLP results as bodies (e.g. persons, places).</rdfs:comment>
        <rdfs:label xml:lang="en">TextSegment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Volume -->


    <owl:Class rdf:about="https://w3id.org/hto#Volume">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Work"/>
        <vann:example>hto:eb7_vol1 a hto:Work, hto:Volume,  schema:CreativeWork;
  hto:number &quot;1&quot;^^xsd:integer;
  hto:title &quot;Seventh edition, Volume 1, Preliminary dissertations&quot;^^xsd:string;
  dct:identifier &quot;192984258&quot;^^xsd:string;
  hto:numberOfPage &quot;942&quot;^^xsd:integer.
  schem:hasPart hto:eb7_vol1_page1.

hto:eb7_vol1_page1 a hto:Work, hto:Page, schema:CreativeWork;
  hto:number  &quot;1&quot;^^xsd:integer;
  schema:image &lt;http://example.com/eb/page001.jpg&gt;.</vann:example>
        <rdfs:comment xml:lang="en">A volume is a part of a successively published historical publication such as a periodical or multi-volume work, often numbered. It may represent a time span, such as a year.</rdfs:comment>
        <rdfs:label xml:lang="en">Volume</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#Work -->


    <owl:Class rdf:about="https://w3id.org/hto#Work">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <vann:example>hto:eb7 a hto:Work hto:Edition;
   hto:mmsid &quot;9910796273804340&quot;^^xsd:string;
   hto:title &quot;Encyclopaedia Britannica&quot;^^xsd:string;
   hto:number &quot;7&quot;^^xsd:integer;
   hto:yearPublished &quot;1842&quot;^^xsd:integer;
   schema:hasPart hto:eb7_vol1.

hto:eb7_vol1 a hto:Work hto:Volume;
  hto:number &quot;1&quot;^^xsd:integer;
  hto:title &quot;Seventh edition, Volume 1, Preliminary dissertations&quot;^^xsd:string;
  dct:identifier &quot;192984258&quot;^^xsd:string;
  hto:numberOfPage &quot;942&quot;^^xsd:integer.</vann:example>
        <rdfs:comment xml:lang="en">A work represents a historical textual work. It can be a book, newspapers, encyclopeadia, or collections of works.</rdfs:comment>
        <rdfs:label xml:lang="en">Work</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hto#WorkCollection -->


    <owl:Class rdf:about="https://w3id.org/hto#WorkCollection">
        <rdfs:subClassOf rdf:resource="https://w3id.org/hto#Work"/>
        <vann:example>hto:eb a hto:Work hto:WorkCollection;
   rdfs:label &quot;Encyclopaedia Britannica Collection&quot;^^xsd:string;
   schema:hasPart hto:eb7.

hto:eb7 a hto:Work hto:Edition;
