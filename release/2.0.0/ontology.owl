        <rdfs:domain rdf:resource="https://w3id.org/hto#ConceptRecord"/>
        <rdfs:range rdf:resource="https://w3id.org/hto#AuthorityType"/>
        <rdfs:comment xml:lang="en">Links a ConceptRecord to an AuthorityType indicating the kind of authority or knowledge base used to identify or describe it (for example, “Wikidata item” or “DBpedia resource”). This property specialises crm:P2_has_type for classifying authority links.</rdfs:comment>
        <rdfs:label xml:lang="en">hasAuthorityType</rdfs:label>
    </owl:ObjectProperty>
    


