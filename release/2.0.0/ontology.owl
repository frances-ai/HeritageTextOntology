        <rdfs:range rdf:resource="https://w3id.org/hto#ConceptRecord"/>
        <rdfs:comment xml:lang="en">Links a skos:Concept to a ConceptRecord that documents how that concept is described in a particular source or system.</rdfs:comment>
        <rdfs:label xml:lang="en">hasConceptRecord</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hto#hasDescription -->


    <owl:ObjectProperty rdf:about="https://w3id.org/hto#hasDescription">
